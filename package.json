{
  "name": "cloudflare-vercel-monitor",
  "displayName": "Cloudflare Vercel Monitor",
  "publisher": "RitabanGhosh",
  "icon": "icon.png",
  "description": "Monitor Cloudflare Pages and Vercel build status inside VS Code.",
  "version": "1.0.2",
  "engines": {
    "vscode": "^1.98.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ritaban06/cloudflare-vercel-monitor.git"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "buildStatus",
          "title": "Build Status",
          "icon": "media/logs.svg"
        }
      ]
    },
    "views": {
      "buildStatus": [
        {
          "id": "buildStatusView",
          "name": "Cloudflare & Vercel Builds",
          "icon": "media/logs.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.openBuildStatus",
        "title": "Refresh Build Status"
      }
    ]
  },
"main": "./dist/extension.js",
"scripts": {
  "build": "webpack",
  "package": "vsce package"
},
  "dependencies": {
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@vscode/test-cli": "^0.0.10",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
